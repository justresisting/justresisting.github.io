/******* VARIABLES *******/ 

:root {
  /* ###Call to Actions */
  --cta-padding: 13px 14px!important;
  --cta-border-radius: 4px!important;
  --cta-border: none!important;
  --cta-shadow: none!important;
  /* ###Callouts */
  --callout-padding: 15px 20px 15px 15px!important;
  --callout-border: 1px solid var(--color-border-default)!important;
  --callout-border-radius: 4px!important;
  --callout-shadow: none!important;
  --callout-icon-size: 20px!important;
  --callout-emoji-size: 18px!important;
  /* ###Collections */
  --collection-list-item-border-radius: 0px!important;
  --collection-list-item-padding: 5px 0px 5px 5px!important;
  --collection-gallery-gap: 30px!important;
  --collection-title-text-size: 16px!important;
  --collection-property-text-size: 14px!important;
  --collection-card-border-radius: 5px!important;
  --collection-card-padding: 20px!important;
  --collection-card-border: 1px solid var(--color-border-default)!important;
  --collection-card-cover-size-small: 172px!important;
  --collection-card-cover-size-medium: 260px!important;
  --collection-card-cover-size-large: 320px!important;
  /* ###Pills [One Line Callouts] */
  --notion-pill-padding: 2px 5px!important;
  --notion-pill-border-radius: 0px!important;
  --notion-pill-text-size: 11px!important;
  --notion-pill-text-weight: 500!important;
  --notion-pill-text-transform: uppercase!important;
  --notion-pill-letter-spacing: 1px!important;
  /* ###Images */
  --image-border-radius: 10px!important;
  /* ###Misc */
  --divider-margin: 15px 0px!important;
  --divider-opacity: 1!important;
  /* ###Typography */
  --text-spacing: 1px!important;
  --secondary-font: 'Inter', sans-serif!important;
  --notion-text-margin: 0px 0px 0px 0px!important;
  /* ###h1 */
  --h1-size: 3em!important;
  --h1-weight: 800!important;
  --h1-spacing: -1px!important;
  --h1-margin: 5px 0px 5px 0px!important;
  --h1-line-height: 1!important;
  /* ###h2 */
  --h2-size: 2rem!important;
  --h2-weight: 700!important;
  --h2-spacing: -0.5px!important;
  --h2-margin: 5px 0px 5px 0px!important;
  --h2-line-height: 1.2!important;
  /* ###h3 */
  --h3-size: 1.375rem!important;
  --h3-weight: 600!important;
  --h3-spacing: -0.5px!important;
  --h3-margin: 10px 0px 10px 0px!important;
  --h3-line-height: 1.4!important;
  /* ###Links */
  --link-border-width: 0px 0px 0px 0px!important;
  --link-border-style: none!important;
  --link-border-color: none !important;
  /* ###Colors */
  /* --color-default is the default theme color */
  /* This affects text color exclusively (right?)
     and gray contrasted poorly with the background 
     color so it was set to white */ 
  --color-default: 10, 49, 97 !important; /* rgb(10,49,97) */
  --color-white: 255, 255, 255 !important; /* rgb(255,255,255) */ 
  --color-gray: 0, 0, 0 !important; /* rgb(0,0,0) */
  --color-black: 0, 0, 0 !important; /* rgb(0,0,0) */
  --color-brown: 101, 67, 33 !important; /* rgb(101,67,33) */
  --color-orange: 255, 119, 0 !important; /* rgb(255,119,0) */
  --color-yellow: 232, 232, 0 !important; /* rgb(232,232,0) */
  --color-green: 0, 202, 0 !important; /* rgb(0,202,0) */
  --color-blue: 51,102,153 !important; /* rgb(51,102,153) */
  --color-purple: 121,85,121 !important; /* rgb(121,85,121) */
  --color-pink: 255,141,223 !important; /* rgb(255,141,223) */
  --color-red: 179, 25, 66 !important; /* rgb(179,25,66) */
  /* ###Text */
  /* Site Foreground */
  /* --color-text-default sets default text color site-wide */
  --color-text-default-light: rgba(var(--color-white),1) !important;
  --color-text-default: rgba(var(--color-white),1) !important;
  --color-text-gray: rgba(var(--color-white),1) !important;
  --color-text-black: rgba(var(--color-black),1) !important;
  --color-text-brown: rgba(var(--color-brown),1) !important;
  --color-text-orange: rgba(var(--color-orange),1) !important;
  --color-text-yellow: rgba(var(--color-yellow),1) !important;
  --color-text-green: rgba(var(--color-green),1) !important;
  --color-text-blue: rgba(var(--color-blue),1) !important;
  --color-text-purple: rgba(var(--color-purple),1) !important;
  --color-text-pink: rgba(var(--color-pink),1) !important;
  --color-text-red: rgba(var(--color-red),1) !important;
  /* ###Site Background */
  /** --color-bg-default sets background color SITE WIDE */
  --color-bg-default: 10, 49, 97 !important; /* rgb(10,49,97) */
  --color-bg-gray: rgba(var(--color-black),.75) !important;
  --color-bg-black: rgba(var(--color-black),.75) !important;
  --color-bg-brown: rgba(var(--color-brown).75) !important;
  --color-bg-orange: rgba(var(--color-orange),.75) !important;
  --color-bg-yellow: rgba(var(--color-yellow),.75) !important;
  --color-bg-green: rgba(var(--color-green),.75) !important;
  --color-bg-blue: rgba(var(--color-blue),.75) !important;
  --color-bg-purple: rgba(var(--color-purple),.75) !important;
  --color-bg-pink: rgba(var(--color-pink),.75) !important;
  --color-bg-red: rgba(var(--color-red),.85) !important;
  /* ? Background Light  What does this do? */
  --color-bg-gray-light: rgba(var(--color-gray),.25) !important;
  --color-bg-brown-light: rgba(var(--color-brown),.75) !important;
  --color-bg-orange-light: rgba(var(--color-orange),.5) !important;
  --color-bg-yellow-light: rgba(var(--color-yellow),.5) !important;
  --color-bg-green-light: rgba(var(--color-green),.5) !important;
  --color-bg-blue-light: rgba(var(--color-blue),.5) !important;
  --color-bg-purple-light: rgba(var(--color-purple),.75) !important;
  --color-bg-pink-light: rgba(var(--color-pink),.75) !important;
  --color-bg-red-light: rgba(var(--color-red),.85) !important;
  /* ###Site Border Colors */
  --color-border-default-light: rgba(var(--color-white),1) !important;
  --color-border-default: rgba(var(--color-white),1) !important;
  --color-border-black:  rgba(var(--color-black),1) !important;
  --color-border-gray: rgba(var(--color-white),1) !important;
  --color-border-brown: rgba(var(--color-brown),1) !important;
  --color-border-orange: rgba(var(--color-orange),1) !important;
  --color-border-yellow: rgba(var(--color-yellow),1) !important;
  --color-border-green: rgba(var(--color-green),1) !important;
  --color-border-blue: rgba(var(--color-blue),1) !important;
  --color-border-purple: rgba(var(--color-purple),1) !important;
  --color-border-pink: rgba(var(--color-pink),1) !important;
  --color-border-red: rgba(var(--color-red),1) !important;
  /* Pill */
  --color-pill-default: rgba(var(--color-default),.5) !important;
  --color-pill-gray: rgba(var(--color-gray),.50) !important;
  --color-pill-brown: rgba(var(--color-brown),.5) !important;
  --color-pill-orange: rgba(var(--color-orange),.5) !important;
  --color-pill-yellow: rgba(var(--color-yellow),.75) !important;
  --color-pill-green: rgba(var(--color-green),.5) !important;
  --color-pill-blue: rgba(var(--color-blue),.5) !important;
  --color-pill-purple: rgba(var(--color-purple),.75) !important;
  --color-pill-pink: rgba(var(--color-pink),.75) !important;
  --color-pill-red: rgba(var(--color-red),0.75) !important;
  /** UI */
  --color-ui-hover-bg: rgba(var(--color-card-bg),0.5) !important;
  --color-ui-hover-bg-light: rgba(var(--color-white),0.5) !important; 
  --color-accent-bg: rgba(var(--color-red),1) !important;
  --color-card-bg: 51, 102, 152 !important;    
  --color-border-default: rgba(var(--color-white), 1) !important;
  --color-border-transparent: transparent !important;
  --color-transparent: transparent !important;
}

/* END VARIABLES */


/* BEGIN POLITICAL CUSTOMIZATION */



/******* #HTML *******/

html {
  font-size: var(--body-font) !important;  
}

body {
  font-size: var(--body-font) !important;
}



/******* #SUPER *******/

.super-content {
  padding-bottom: 4em !important;
}



/* #NAVBAR */

.super-navbar {
  -webkit-backdrop-filter: blur(16px) !important;
  backdrop-filter: blur(16px) !important;
  max-width: var(--theme-width) !important;
  margin: 1em auto  !important;
  width: calc(100% - var(--column-spacing)) !important;
  border-radius: var(--element-radius) !important;
  top: 1em !important;
}

.super-navbar__cta {
  padding: 0.75em 2em !important;
  border-radius: var(--element-radius) !important;
  font-size: 0.8rem !important;
  transition: 0.1s ease-in-out !important;
}

.super-navbar__content {
  padding-left: 1em !important;
  padding-right: 1em !important;
}

.super-navbar__actions, 
.super-navbar__logo {
  padding: 0 !important;
}



/******* ###NOTION HEADER *******/

.notion-header__title-wrapper {
  max-width: var(--theme-width) !important;
}

.notion-header__content.max-width {
  max-width: var(--theme-width) !important;
}

.notion-header__cover {
  margin: 1em 1em 4em 1em !important;
  max-width: var(--theme-width) !important;
  margin: auto !important;
  width: calc(100% - var(--column-spacing)) !important;
  border-radius: var(--element-radius) !important;
}

.notion-header__cover.no-cover {
  max-height: 4em !important;
  height: 0 !important; 
}

.notion-heading {
  padding: 0 !important;
}

.notion-column {
  margin-bottom: 1em !important;
  padding: 0 !important;
}


/******* CTA *******/

.highlighted-background {
  transition: 0.3s ease-in-out !important;
  border-radius: 2px !important;
}

.highlighted-background:hover{
  opacity: 0.8 !important;
}

.highlighted-background a{
  padding: 0.9em 1.5em !important;
  border: none !important;
  opacity: 1 !important;
  border-radius: var(--element-radius) !important;
  color: var(--color-bg-default) !important;
}

.highlighted-background.bg-gray a{
  background-color: var(--color-black) !important;
}

.highlighted-background.bg-brown a{
  background-color: var(--color-brown) !important;
}

.highlighted-background.bg-orange a{
  background-color: var(--color-orange) !important;
}

.highlighted-background.bg-yellow a{
  background-color: var(--color-yellow) !important;
}

.highlighted-background.bg-green a{
  background-color: var(--color-green) !important;
}

.highlighted-background.bg-blue a{
  background-color: var(--color-blue) !important;
}

.highlighted-background.bg-purple a{
  background-color: var(--color-purple) !important;
}

.highlighted-background.bg-pink a{
  background-color: var(--color-pink) !important;
}

.highlighted-background.bg-red a{
  background-color: var(--color-red) !important;
}




