}
/* .super-content affects the header padding left behind*/
/* END Show/Hide Notion Header*/


/* Show/Hide All Notion Page Block Icons  */
.notion-icon, 
.notion-icon__page, 
.notion-header__icon,
.notion-page__icon,
.notion-header__icon-wrapper { 
  display: block !important;
/* was set to display: none */
}

/* Show/Hide Column Header for Board Database */
.notion-collection-board__column-header {
  display: block !important;
/* was set to display: none */
}

/* Show/Hide Database Name */
.notion-collection__header-wrapper {
  display: block !important;
}

/* Show/Hide Notion Database Header Icon */
.notion-collection__header-wrapper {
display: block !important;
/* was set to display: none */
}

/* Show/Hide Icon on Database Card  */
.notion-property,
.notion-property__title,
.notion-property__title__icon-wrapper 
.notion-property .notion-property__title {
display: block !important;
}

/* Show/Hide Page Title */
.notion-header__title {
display: block !important;
/* was set to display: none */
}

.notion-header__title-wrapper {
display: block !important;
/* was set to display: none */
}

/* Show/Hide Page Icon image */
.notion-header__icon-wrapper {
display: block !important;
/* was set to display: none */
}

.notion-callout__icon {
display: none  !important;
}

/* SHOW/Hide All Toggles*/
.notion-toggle {
display: block !important;
}
/* HIDE All Toggles with Brown Background */
/* Use these to hide page content*/
.notion-toggle.bg-brown {
display: none !important;
}

/* Set Page Width */
.super-content.max-width {
max-width: var(--theme-width) !important;
margin: auto !important;
}



/* Global Variables */
:root {
/* Global Border Radius [Rounding] */
--element-radius: 15px !important;
/* Desktop Page Width */
--theme-width: 1440px !important;
/* Site Font Size*/
--body-font: 16px !important;
/* Site Column Spacing*/
--column-spacing: 2em !important;
/* Site Background */
--color-bg-default: rgb(10, 49, 97) !important; 
/* UI COLORS */
--color-card-bg: rgb(37, 77, 117) !important;
/* Collection Card Size*/
--collection-card-cover-size-small: calc(20%) !important;
--collection-card-cover-size-medium: calc(30%) !important;
--collection-card-cover-size-large: calc(45%) !important;
}

/******* END #COLORS# *******/
/******* END #VARIABLES *******/

/******* #MOBILE RESPONSIVE *******/


@media screen and (max-width: 880px) {


  h1 {
     font-size: 2rem !important;
 }
  h2 {
     font-size: 1.5rem !important;
 }
  h3 {
     font-size: 1.25rem !important;
 }


.notion-divider {
 border-bottom: none !important;
 margin: 2em 0 !important;
}

.notion-column {
 margin-bottom: 0 !important;
 padding: 0 !important;
}

.notion-quote {
 padding: 2em !important;
}

.notion-collection-list__item-content {
