/**  START #Callout Button */ 
/** (Entire block is clickable) */
/** Set Notion BG Color on Callout */
.notion-callout { 
  position: relative!important;
}

/*.notion-callout .notion-callout__content:first-of-type > .notion-semantic-string span .notion-link  */
.notion-callout.__content:first-of-type > .notion-semantic-string.span.notion-link {
  /* Basic Button Style */
  box-sizing: border-box !important;
  appearance: none !important;
  background-color: transparent !important;
  border: 2px solid rgb(var(--color-border-transparent)) !important;
  border-radius: 0.6em !important;
  color: white !important;
  cursor: pointer !important;
  display: flex !important;
  margin: 20px !important;
  padding: 1.2em 2.8em !important;
  align-self: center !important;
  text-align: center !important;
  text-decoration: none !important;
  text-transform: uppercase !important;
  font-family: 'Montserrat', sans-serif !important;
  font-weight: 700 !important;
  font-size: 1rem !important;
  line-height: 1 !important;
}

  /** Default Callout Button - BG color in Notion */  
  [class="notion-callout border"] {
    color: white !important;
    background-color: var(--color-card-bg) !important;
