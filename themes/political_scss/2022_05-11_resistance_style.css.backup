/*******  #TO_DO *******/


.notion-to-do__content {
    margin-bottom: 1em!important;
}


.notion-to-do__icon {
  margin-inline: 0px!important;
  margin-right: 16px!important;
}

.notion-checkbox{
  width:1.5em!important;
  height: 1.5em!important;
  border-radius: var(--element-radius)!important;
  background-color: var(--color-bg-default)!important;
  border: 1px solid var(--color-text-default)!important;
}

.notion-checkbox svg{
  display: none!important;
}

.notion-checkbox.checked {
  width:1.5em!important;
  height: 1.5em!important;
  border-radius: var(--element-radius)!important;
  background-color: var(--color-text-default)!important;
  border: 1px solid var(--color-text-default)!important;
}

.notion-checkbox.checked svg{
  display: inline!important;
}

.notion-checkbox.checked > svg {
  fill: var(--color-bg-default)!important;
}

.notion-to-do__title  {
  opacity: 1!important;
  text-align: left!important;
}

.notion-to-do__title.checked  {
  opacity: 1!important;
  text-align: left!important;
}

.notion-to-do__title.checked del {
  text-decoration: none!important;
  text-align: left!important;
}






/*******  CALLOUT *******/



p.notion-callout__content{
  max-width: var(--theme-width)!important;
  width: var(--theme-width)!important;
}


.notion-callout {
  border-radius: var(--element-radius)!important;
  border: 1px solid var(--color-border-transparent)!important;
  padding: 1.5em 1.5em 0.5em 1.5em!important;
  overflow: hidden!important;
  margin-bottom: 1em!important;
}

.notion-callout.bg-gray-light, 
.notion-callout.bg-brown-light, 
.notion-callout.bg-orange-light, 
.notion-callout.bg-yellow-light, 
.notion-callout.bg-green-light, 
.notion-callout.bg-blue-light, 
.notion-callout.bg-purple-light, 
.notion-callout.bg-pink-light, 
.notion-callout.bg-red-light {
  border:none!important;
}

.notion-page {
  border: 1px solid var(--color-border-transparent)!important;
  border-radius: var(--element-radius)!important;
}

.notion-page > div {
  border-radius: var(--element-radius)!important;
  display: flex;
  transition: 300ms ease-in!important;
  padding: 0.5em!important;
}

.notion-page__title .notion-semantic-string {
  border-bottom: none var(--color-border-transparent);
}




/*******  #BOOKMARKS *******/

.notion-bookmark__description {
  margin: 0;
  margin-bottom: 0px;
  font-size: 12px;
  line-height: 1rem;
  height: fit-content!important;
  overflow: hidden;
}

/*******  #TOGGLES *******/


.notion-toggle {
  margin-bottom: 1rem!important;
}

.notion-toggle__summary {
  font-weight: 600!important;
}

.notion-pill {
  padding: 0.25em 0.75em!important;
  border-radius: var(--element-radius)!important;
}

.notion-toggle__summary::before {
  font-size: 2em!important;
  border-radius: var(--element-radius)!important;
  margin: 0 0em 0.25em 0!important;
  position: relative!important;
}

.notion-bulleted-list
.notion-numbered-list, 
.notion-collection-card {
    color: var(--color-text-white);
    background-color: rgba(var(--color-card-bg), .25);
    font-family: var(--secondary-font)!important;
}

.notion-collection-card {
  box-shadow: none!important;
  border: none;
  border-radius: var(--element-radius)!important;
  transition: .3s ease-in-out !important;
}

.notion-collection-card:hover {
  color: var(--color-text-white);
  background-color: rgba(var(--color-card-bg),.75)!important;
  transform: scale(1.1)!important;
  border: none!important;
  box-shadow: 0px 24px 16px 0px rgba(0,0,0,0.05)!important;
  -webkit-box-shadow: 1px 17px 23px 0px rgba(0,0,0,0.05)!important;
  -moz-box-shadow: 1px 17px 23px 0px rgba(0,0,0,0.05)!important;
}

.notion-collection-card__cover img{
  border-radius: 0px!important;
  border: 0!important;
}

.notion-collection-card__content {
  padding: 0em 1rem 1rem 1rem!important;
}

.notion-collection-board {
  gap: 1rem!important;
  border: none!important;
  overflow-x: scroll!important;
  margin-bottom: 1rem!important;
  padding: 1em!important;
  background-color: var(--color-border-transparent)!important;
  border-radius: var(--element-radius)!important;
}

.notion-collection-board__item {
  margin-bottom: 1rem!important;
}

.notion-collection-board .notion-property__title,  
.notion-collection-gallery .notion-property__title {
  padding: 1rem!important;
  width: fit-content!important;
}

.super-content {
  padding-bottom: 4em!important;
}

.notion-collection-card__cover.large {
  height: 18rem!important;
  max-height: 18rem!important;
}

.notion-collection-card__cover.medium {
  height: 12rem!important;
  max-height: 12rem!important;
}

.notion-collection-card__cover.small {
  height: 10rem!important;
  max-height: 10rem!important;
}

.notion-collection-gallery {
  display: grid;
  grid-auto-rows: 1fr;
  gap: var(--column-spacing)!important;
  padding: 0em!important;
  border: none!important;
  margin-bottom: 1rem!important;
  border-radius: var(--element-radius)!important;
}

.notion-property__title .notion-property__title__icon-wrapper {
    display: block!important;
}

.notion-collection-card img {
  opacity: 1!important;
  transition: 0.3s ease-in-out!important;
}

.notion-collection-card:hover img {
  transform: scale(1.1) !important;
}

.notion-collection-card.gallery {
  border: 1px solid var(--color-border-transparent)!important;
  border-radius: var(--element-radius)!important;
  transition: .3s ease-in-out !important;
}

.notion-collection-card.gallery:hover {
  border: 1px solid var(--color-bg-default)!important;
  border-radius: var(--element-radius)!important;
  background-color: var(--color-card-bg)!important;
  transition: .3s ease-in-out !important;
}





/*******  Quotes  *******/


.notion-quote {
  font-style: italic!important;
  font-size: 1em!important;
  font-weight: 500!important;
  width: 100% !important;
  margin: 1rem auto!important;
  padding: 1em 1em 2em 2em!important;
  border: 1px solid var(--color-border-default)!important;
  text-align: center!important;
  display: flex!important;
  flex-direction: column!important;
  align-items: center!important;
  border-radius: var(--element-radius)!important;
}

.notion-quote.bg-gray, 
.notion-quote.bg-brown, 
.notion-quote.bg-orange, 
.notion-quote.bg-yellow, 
.notion-quote.bg-green, 
.notion-quote.bg-blue, 
.notion-quote.bg-purple, 
.notion-quote.bg-pink, 
.notion-quote.bg-red {
  /* border:none!important; */
  border: 1px solid var(--color-border-default)!important;
}





/*******  #TABLE DATABASE *******/


.notion-collection-table {
  font-size: 1rem!important;
  border: 1px solid var(--color-border-transparent)!important;
  border-radius: var(--element-radius)!important;
  overflow: hidden!important;
  padding: 1em!important;
}

.notion-collection-table__wrapper{
  background-color: var(--color-border-transparent)!important;
  padding: 1em!important;
  border-radius: var(--element-radius)!important;
}

.notion-collection-table td, 
.notion-collection-table th {
  padding: 1rem!important;
  border: 1px solid var(--color-border-default)!important;
  overflow: hidden!important;
  background-color: var(--color-bg-default)!important;
}

.notion-collection-table__head {
  background-color: var(--color-ui-hover-bg-light)!important;
}

.notion-collection-table__cell {
  padding: 1rem!important;
}

.notion-collection-table__cell .notion-pill {
  margin-bottom: 0.5rem!important;
}

.notion-collection-table__cell.title .notion-semantic-string {
  white-space: normal!important;
}





/*******  #Simple TABLE *******/


.notion-table {
  font-size: 1rem!important;
  margin: 1rem 0!important;
  overflow: hidden!important;
  padding: 1em!important;
}


.notion-table td, 
.notion-table th {
  padding: 1rem!important;
  overflow: hidden!important;
}



/*******  #TABLE OF CONTENTS  *******/



.notion-table-of-contents {
  padding: 1rem!important;
  font-size: 0.75rem!important;
  margin: 1rem 0!important;
  border-radius: var(--element-radius)!important;
  z-index: 19!important;
  border: 1px solid var(--color-border-transparent)!important;
  color: var(--color-white);
  /*background: var(--color-card-bg)!important;*/
  background-color: transparent!important;
  opacity: 1!important;
}

.notion-table-of-contents__item {
  padding: 0.25em 0.5em!important;
  opacity: 1!important;
  transition: .5s!important;
}

.notion-table-of-contents__item .notion-semantic-string {
  border-bottom: none!important;
}

.notion-table-of-contents.bg-yellow, 
.notion-table-of-contents.bg-yellow, 
.notion-table-of-contents.bg-orange, 
.notion-table-of-contents.bg-gray, 
.notion-table-of-contents.bg-brown, 
.notion-table-of-contents.bg-green, 
.notion-table-of-contents.bg-blue, 
.notion-table-of-contents.bg-purple, 
.notion-table-of-contents.bg-pink,
.notion-table-of-contents.bg-red{
  border: none!important;
}

.notion-table-of-contents__item:hover {
  background-color: none!important;
  opacity: 0.5!important;
}





/*******  #LIST   *******/


.notion-collection-list {
  border: none!important;
  padding: 1rem!important;
  background-color: var(--color-border-transparent)!important;
  border-radius: var(--element-radius)!important;
}

.notion-collection-list__item {
  background-color: var(--color-bg-default)!important;
  padding: 1rem!important;
  transition: 0.3s !important;
  margin-bottom: 0.5rem!important;
}

.notion-collection-list__item:hover {
  background-color: var(--color-bg-default)!important;
  box-shadow: 0px 24px 16px 0px rgba(0,0,0,0.05)!important;
  -webkit-box-shadow: 1px 17px 23px 0px rgba(0,0,0,0.05)!important;
  -moz-box-shadow: 1px 17px 23px 0px rgba(0,0,0,0.05)!important;
  transform: scale(1.1)!important;
}

.notion-collection-list__item .notion-property__title .notion-semantic-string > span {
border-bottom: none!important;
white-space: normal!important;
}

.notion-collection-list__item-property {
  margin-left: 0.5rem!important;
  font-size: 0.75rem!important;
}

.notion-collection-list__item-property .notion-property__date, 
.notion-collection-list__item-property .notion-property__select .notion-pill {
  font-size: 1em!important;
}

.notion-semantic-string .individual span{
  padding-left:0.5em!important;
}

.notion-semantic-string .individual {
  display: flex!important;
  font-size: 1em!important;
}




/******* #Calendar *******/

.notion-collection-calendar {
  border: none!important;
  padding: 1rem!important;
  background-color: var(--color-border-transparent)!important;
  border-radius: var(--element-radius)!important;
}

.notion-collection-calendar__week-days {
    box-shadow: none!important;
}

.notion-collection-calendar__row {
    box-shadow: none!important;
}

.notion-collection-calendar__date {
  background-color: var(--color-bg-default)!important;
  border-color:  var(--color-border-default)!important;;
}

.notion-collection-card.calendar .notion-collection-card__content {
    padding: 0px 6px 1px 6px!important;
}



/* END TEMPLATE STARTER */




/* OVERWRITES */


/* AMETECH OVERWRITES */

/* Show/Hide Notion-Navbar  */
.notion-navbar {
    display: none !important;
}

/* Show/Hide Notion Page Blocks */
.notion-page {
    display: block!important;
}

/* Set Page Block BG Color*/
.notion-page {  
    background-color: rgba(var(--color-card-bg), .75)!important;
}

/* Increase/Decrease Opacity to change Page BG Color on Mouseover */
.notion-page:focus,
.notion-page:hover {
background-color: rgba(var(--color-card-bg), .90);
}

/* HIDE All Notion Page Block Icons  */
.notion-icon, 
.notion-icon__page, 
.notion-header__icon,
.notion-page__icon,
.notion-header__icon-wrapper { 
    display: block!important;
/* was set to display: none */
}

/* Show/Hide Column Header for Board Database */
.notion-collection-board__column-header {
    display: block!important;
/* was set to display: none */
}

  
/* Show/Hide Database Name */
.notion-collection__header-wrapper {
    display: block!important;
}
/* Show/Hide Notion Database Header Icon */
.notion-collection__header-wrapper {
  display: block!important;
/* was set to display: none */
}

/* Show/Hide Icon on Database Card  */
.notion-property,
.notion-property__title,
.notion-property__title__icon-wrapper 
.notion-property .notion-property__title {
  display: flex!important;
}

/* Show/Hide Page Title */
.notion-header__title {
  display: block!important;
/* was set to display: none */
}

.notion-header__title-wrapper {
  display: block!important;
/* was set to display: none */
}

/* Show/Hide Page Icon image */
.notion-header__icon-wrapper {
  display: block!important;
/* was set to display: none */
}

.notion-callout__icon {
  display: none !important;
}

/* Show/Hide ALL TOGGLES */
.notion-toggle,
.notion-toggle_summary {
  display: block!important;
}


/* Global Variables */
:root {
  /* Global Border Radius [Rounding] */
  --element-radius: 25px!important;
  /* Desktop Page Width */
  --theme-width: 1440px!important;
  /* Site Font Size*/
  --body-font: 20px!important;
  /* Site Column Spacing*/
  --column-spacing: 2em!important;
  /* Site Background */
  --color-bg-default: rgb(51, 102, 153)!important; 
  /* UI COLORS */
  --color-card-bg: rgb(37, 77, 117)!important;
  /* Collection Card Size*/
  --collection-card-cover-size-small: calc(20%)!important;
  --collection-card-cover-size-medium: calc(30%)!important;
  --collection-card-cover-size-large: calc(45%)!important;
}

/******* END #OVERWRITES# *******/





